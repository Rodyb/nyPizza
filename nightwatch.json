{
  "src_folders": [
    "tests"
  ],
  "output_folder": "reports",
  "custom_commands_path": "commands",
  "page_objects_path": "page_objects",
  "custom_assertions_path": "",
  "globals_path": "./globals.js",
  "live_output": false,
  "parallel_process_delay": 10,
  "disable_colors": false,
  "test_workers": false,
  "reporter": "./html-reporter.js",
  "selenium": {
    "start_process": false,
    "server_path": "",
    "log_path": "",
    "host": "127.0.0.1",
    "port": 4444,
    "cli_args": {
    }
  },
  "test_settings": {
    "default": {
      "launch_url": "https://www.newyorkpizza.nl/",
      "selenium_port": 9515,
      "selenium_host": "localhost",
      "silent": true,
      "output": true,
      "default_path_prefix": "",
      "detailed_output": false,
      "screenshots": {
        "enabled": true,
        "path": "./reports",
        "on_failure": true,
        "on_error": true
      },
      "globals": {
        "userName": "newUser"
      },
      "desiredCapabilities": {
        "browserName": "chrome",
        "javascriptEnabled": true,
        "acceptSslCerts": true,
        "chromeOptions": {
          "args":
          ["start-maximized"]
        }
      }
    },
    "localEnvironment": {
      "launch_url": "",
      "globals": {
        "vivanteUserName": "",
        "vivantePassword": "",
        "administrator_username": "",
        "administrator_password": ""
      }
    },
    "daily": {
      "launch_url": "",
      "globals": {
        "evision_username": "",
        "evision_password": "",
        "administrator_username": "",
        "administrator_password": ""
      }
    },
    "mobileChrome": {
      "desiredCapabilities": {
        "browserName": "chrome",
        "chromeOptions": {
          "mobileEmulation": {
            "deviceName": "Apple iPhone 5"
          }
        }
      }
    },
    "chrome": {
      "desiredCapabilities": {
        "browserName": "chrome"
      },
      "ie": {
        "desiredCapabilities": {
          "browserName": "ie"
        }
      }
    }
  }
}
